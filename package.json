{
  "name": "@devlikeapro/n8n-nodes-waha",
  "version": "2024.9.5",
  "description": "n8n to connect with WAHA (Whatsapp HTTP API)",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "https://n8n.io",
  "author": {
    "name": "WAHA",
    "email": "waha@devlike.pro"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devlikeapro/n8n-nodes-waha.git"
  },
  "moduleDirectories": [
    "node_modules",
    "src"
  ],
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "test": "jest",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/WAHAApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/WAHA/WAHA.node.js",
      "dist/nodes/WAHA/WAHATrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/express": "^4.17.6",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.5.5",
    "@types/request-promise-native": "~1.0.15",
    "@typescript-eslint/parser": "~5.45",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-core": "*",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "ts-jest": "^29.2.5",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "@devlikeapro/n8n-openapi-node": "^0.1.0",
    "lodash": "^4.17.21",
    "openapi-types": "^12.1.3",
    "pino": "^9.4.0"
  }
}
